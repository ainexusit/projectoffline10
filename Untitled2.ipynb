    "import re\n",
    "\n",
    "my_string = input(\"Enter the string :\")\n",
    "pattern = r'^YN[a-zA-Z].*'\n",
    "if re.match(pattern, my_string):\n",
    "    print(\"Starts with YN\")\n",
    "else:\n",
    "    print(\"not found\")"
